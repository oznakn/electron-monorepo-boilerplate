{
    "compilerOptions": {
        "composite": true,
        "module": "commonjs",
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "incremental": true,
        "target": "ESNext",
        "lib": [
            "ESNext",
        ],
        "strict": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
    },
}
